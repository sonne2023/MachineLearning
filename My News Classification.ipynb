   "cell_type": "code",
   "execution_count": 65,
   "metadata": {},
   "outputs": [],
   "source": [
    "newsgroups_test['data_stemming'] = \\\n",
    "                            stemming_texts(newsgroups_test.preprocess_data)"
   ]
